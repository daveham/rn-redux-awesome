{
  "name": "AwesomeProject",
  "version": "0.0.1",
  "private": true,
  "description": "Adding redux/sagas to standard react native learning app",
  "author": "daveham",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:daveham/rn-redux-awesome.git"
  },
  "scripts": {
    "android": "react-native run-android",
    "android:tron": "adb reverse tcp:9090 tcp:9090 && react-native run-android",
    "ios": "react-native run-ios --simulator=\"iPhone 11\"",
    "ios:release": "react-native run-ios --simulator=\"iPhone 11\" --configuration Release",
    "ios:device": "react-native run-ios --device",
    "ios:device:release": "react-native run-ios --device --configuration Release",
    "ios:dave:debug": "react-native run-ios --udid put-your-phone-id-here",
    "ios:dave:release": "react-native run-ios --udid put-your-phone-id-here --configuration Release",
    "ios:build": "react-native run-ios --configuration Release --no-packager",
    "ios:x": "FORCE_BUNDLING=true react-native run-ios --no-packager --simulator=\"iPhone 11\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clear-rn-cache": "npm start -- --reset-cache"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.2",
    "axios": "^0.19.0",
    "react": "16.9.0",
    "react-native": "0.62.3",
    "react-native-paper": "^2.16.0",
    "react-native-vector-icons": "^6.6.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "eslint": "^6.4.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-test-renderer": "16.9.0",
    "reactotron-react-native": "^4.0.2",
    "reactotron-redux": "^3.1.2",
    "reactotron-redux-saga": "^4.2.2",
    "remote-redux-devtools": "^0.5.16"
  },
  "jest": {
    "preset": "react-native"
  }
}
